
<% form_for @post, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title%>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.file_field :photo%>
  </p>
  <p><%= f.submit "Dodaj" %></p>
<!--  <%= render :partial => "form", :object => f %>-->
  <%= link_to 'Wróć', posts_path %>
<% end %>
